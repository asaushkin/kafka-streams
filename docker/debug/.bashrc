PROMPT_COMMAND='history -a'

bind '"\e[1;5D" backward-word'
bind '"\e[1;5C" forward-word'

alias ll='ls -alF --color=auto'

GIT_PROMPT_ONLY_IN_REPO=0
GIT_PROMPT_THEME=Evermeet

